{
  "name": "test",
  "version": "1.0.0",
  "description": "test",
  "license": "UNLICENSED",
  "main": "./server/index.js",
  "browser": {
    "winston": false
  },
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=1 yarn build",
    "flow": "flow",
    "flow:install": "flow-typed install",
    "flow:coverage": "flow-coverage-report -i 'app/**/*.js' -x 'app/**/*.test.js' -x 'app/**/*.style.js' -t html -t json -t text -o reports/flow/ --threshold 70",
    "build": "next build ./app && yarn run config-build",
    "clean": "rimraf ./app/.next",
    "config-build": "node ./config/build",
    "dev": "rimraf ./app/.next && yarn install && yarn run dev:serve",
    "dev:serve": "node --icu-data-dir=./node_modules/full-icu ./server/index.js",
    "debug": "rimraf ./app/.next && yarn install && yarn run debug:serve",
    "debug:serve": "nodemon --inspect ./server/index.js",
    "createSitemap": "cross-env SM_ENV=prod node ./sitemap-generator/createsitemap.js",
    "prod": "yarn run build && yarn run prod:serve",
    "prod:serve": "node --icu-data-dir=./node_modules/full-icu ./server/index.js",
    "mock": "canned -p 9876 ./mock",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./app",
    "dev:mock": "yarn install && npm-run-all --parallel mock \"dev:serve -- --mock\"",
    "dev-server": "cross-env BUILD_ENV=dev-server yarn run prod",
    "qa-server": "cross-env BUILD_ENV=qa-server yarn run prod",
    "demo-server": "cross-env BUILD_ENV=demo-server yarn run prod",
    "prod-server": "cross-env BUILD_ENV=prod-server yarn run prod",
    "uat-server": "cross-env BUILD_ENV=uat-server yarn run prod",
    "stage-server": "cross-env BUILD_ENV=stage-server yarn run prod",
    "uatwa-server": "cross-env BUILD_ENV=uatwa-server yarn run prod",
    "lint:js": "eslint app/ server/ --fix",
    "lint:css": "stylelint app/",
    "lint": "yarn run lint:js && yarn run lint:css",
    "test": "cross-env NODE_ENV=test jest --config ./config/jest.config.js",
    "test:async": "yarn run test --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --config ./config/jest.config.js",
    "audit": "yarn run start -- --report",
    "precommit": "yarn run lint",
    "prepush": "yarn run test",
    "generate": "plop --plopfile generators/index.js",
    "size": "size-limit",
    "pa11y": "node ./pa11y"
  },
  "author": "Sapient",
